{
  "name": "matman",
  "version": "0.1.0",
  "description": "mock service and tester assitant",
  "main": "app.js",
  "scripts": {
    "build-css": "node-sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "server": "node server/app",
    "mat": "babel-node ./mat/index.js",
    "test": "mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helinjiang/matman.git"
  },
  "keywords": [
    "mock",
    "tester"
  ],
  "author": "helinjiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/helinjiang/matman/issues"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "express": "^4.15.2",
    "faker": "^4.1.0",
    "fs-extra": "^3.0.1",
    "lodash": "^4.17.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "0.9.5"
  }
}
